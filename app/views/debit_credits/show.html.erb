
<!-- Page Wrapper -->
<div id="wrapper">
    <%= render "home/sidebar" %>
    
    <!-- Content Wrapper -->
    <div id="content-wrapper" class="d-flex flex-column">

        <!-- Main Content -->
        <div id="content">

            <%= render "home/topbar" %>

            <!-- Begin Page Content -->
            <div class="container-fluid">
                <div class="card border-warning">
                <!-- Page Heading -->
                <div class="card-header bg-dark text-light d-sm-flex align-items-center justify-content-between">
                    <h3>Debit/Credit Details</h3>
                    
                </div>

                <div class="card-body">
                      <% if notice %>
                        <p style="color: green"><%= notice %></p>
                      <% end %>
                      <%= render @debit_credit %>                     
                                    
                  
                </div>
              
              </div>                    
                    
                   
               

            </div>
            <!-- /.container-fluid -->

        </div>
        <!-- End of Main Content -->

        <%= render "home/footer" %>

    </div>
    <!-- End of Content Wrapper -->

</div>
<!-- End of Page Wrapper -->





